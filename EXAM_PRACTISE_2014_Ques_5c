#include <stdio.h>
int main(){
  int n,c=0;
  puts("Please enter the size of the array");
  scanf("%d",&n);
  int arr[n];
  for(int i=0;i<n;i++){
    puts("Enter an integer");
    scanf("%d",&arr[i]);
    c += (arr[i]<20)?1:0;
  }
  printf("There are %d numbers that are less than 20\n",c);
  return 0;
}
